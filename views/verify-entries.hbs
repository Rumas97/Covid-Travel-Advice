<h1>User entry verification</h1>

<ul>
{{#each allEntries }}

<li>Status: {{this.status}}</li>

<li>Date: {{this.currentDate}}</li>

<li>Travelling To: {{this.travellingTo}}</li>

<li>Travelling From: {{this.travellingFrom}}</li>

<li>Quarantine: {{this.quarantine}}</li>

<li>COVID test: {{this.covidTest}}</li>

<li>Experience: {{this.experience}}</li>

<form action="/user-entries/{{this.id}}/delete" method="POST">
  <button>Delete</button>
</form>

<form action="/user-entries/{{this.id}}/verify" method="POST">
  <button>Verify</button>
</form>


{{/each}}
</ul>
